{
    "name": "acquia/contenthub-console",
    "description": "A package providing Acquia ContentHub commands for the CommonConsole command line interface.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Acquia Engineering",
            "homepage": "https://www.acquia.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "acquia/acsf-contenthub-console": "^1.5",
        "ext-json": "*",
        "ext-simplexml": "*",
        "spatie/ssl-certificate": "1.21|1.22.1|^2",
        "zumba/amplitude-php": "^1"
    },
    "autoload": {
        "psr-4": {
            "Acquia\\Console\\ContentHub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acquia\\Console\\ContentHub\\Tests\\": "tests",
            "Acquia\\Console\\Cloud\\Tests\\": "vendor/acquia/cloud-contenthub-console/tests",
            "Acquia\\Console\\Acsf\\Tests\\": "vendor/acquia/acsf-contenthub-console/tests",
            "EclipseGc\\CommonConsole\\Tests\\": "vendor/eclipsegc/common-console/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.4",
        "drupal/coder": "dev-8.x-3.x",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.8"
    }
}
